{
  "name": "vbs-gen",
  "version": "1.0.0",
  "description": "Generate VBScript files for fun",
  "main": "lib/index.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json && prettier -c **/*.ts",
    "prettier": "prettier --write **/*.ts",
    "build": "tsc --build tsconfig.build.json",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "npm run build && node lib/index"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "inquirer": "^6.5.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "keywords": [
    "vbs"
  ],
  "author": "Mickole Nulud",
  "license": "MIT"
}
